{
  "name": "react-toolbox-core",
  "version": "0.0.0-alpha.a1564456",
  "description": "A set of factories to generate universal agnostic components",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib --ignore '__tests__'",
    "build:watch": "babel ./src --out-dir ./lib --ignore '__tests__' --watch",
    "prepublish": "npm run build",
    "test": "node --harmony_proxies node_modules/.bin/jest --maxWorkers 4",
    "test:watch": "node --harmony_proxies node_modules/.bin/jest --watch"
  },
  "keywords": [],
  "author": "Javi Velasco <javier.velasco86@gmail.com> (http://javivelasco.com/)",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^1.28.0",
    "inline-style-prefixer": "^2.0.5",
    "ramda": "^0.23.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ~15.4.0",
    "react-dom": "^0.14.0 || ~15.4.0"
  },
  "devDependencies": {
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "jest": "^18.1.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "(\\.css$)|(normalize.css/normalize)|(^exports-loader)": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "../../webpack/webpack.config.dev.js"
        }
      }
    }
  }
}
