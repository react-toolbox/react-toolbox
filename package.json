  "scripts": {
    "babel": "babel ./components --out-dir ./lib",
    "build": "cross-env NODE_ENV=production gulp",
    "clean": "rimraf ./lib",
    "css": "cpx \"./components/**/*.css\" ./lib",
    "lint": "npm run lint:js && npm run lint:css",
